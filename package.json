  },
  "homepage": "https://github.com/1onlinesolution/dws#readme",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
